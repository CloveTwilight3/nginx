# Redirect gaming.clovetwilight3.co.uk → www.mazeymoos.com
server {
    listen 80;
    listen [::]:80;
    server_name gaming.clovetwilight3.co.uk;

    return 301 $scheme://www.mazeymoos.com$request_uri;
}

